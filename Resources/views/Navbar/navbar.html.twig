<nav>
    <div class="da-navbar-wrapper {{ fixedTop|default(false) ? ' navbar-fixed-top' : '' }}">
        <div class="navbar da-navbar-style{{ inverse|default(false) ? ' navbar-inverse' : ' navbar-default' }}{{ staticTop|default(false) ? ' navbar-static-top' : '' }}">
            <div class="container">
                {% block header %}
                    <div class="navbar-header">
                        {% block toggle %}
                            <button type="button" class="navbar-toggle" data-toggle="collapse"
                                    data-target=".navbar-responsive-collapse">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        {% endblock %}
                        {% block brand %}
                            <a class="navbar-brand col-md-1"
                               href="{{ path('justmoov_welcome') }}"
                               alt="Dacorp">
                                {% image '@DacorpExtraBundle/Resources/public/images/logo.png' %}
                                <img src="{{ asset_url }}" alt="Dacorp"/>
                                {% endimage %}</a>
                        {% endblock %}
                    </div>
                {% endblock %}
                {% block menu_container %}
                    <div class="col-md-11 col-xs-12 col-sm-11">
                        <div class="collapse navbar-collapse navbar-responsive-collapse">
                            <div class="row">

                                {% block menu %}

                                    <div class="col-md-6">
                                        {{ mopa_bootstrap_menu('DacorpExtraBundle:MenuBuilder:mainMenu') }}
                                    </div>
                                    <div class="col-md-4">
                                        <div class="row">
                                            {% if is_expr_granted('isAuthenticated()') %}
                                                {{ mopa_bootstrap_menu('DacorpExtraBundle:MenuBuilder:userMenu') }}
                                            {% else %}
                                                {% include "DacorpExtraBundle:Form:navbar-login-form.html.twig" %}
                                            {% endif %}
                                        </div>

                                    </div>
                                    <div class="col-md-2">
                                        {{ mopa_bootstrap_menu('DacorpExtraBundle:MenuBuilder:rightMenu') }}
                                    </div>
                                {% endblock %}
                            </div>
                        </div>
                    </div>
                {% endblock %}
            </div>
        </div>
    </div>
</nav>


